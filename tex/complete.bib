	keywords = {Sensitivity analysis},
	number = {1-3},
	pages = {97 - 113},
	title = {Predicting eutrophication effects in the Burrinjuck Reservoir (Australia) by means of the deterministic model SALMO and the recurrent neural network model ANNA},
	url = {http://www.sciencedirect.com/science/article/B6VBS-44HYNCP-9/2/ab9994b20c0bd170103706ab50de607a},
	volume = {146},

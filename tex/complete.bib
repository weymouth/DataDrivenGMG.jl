	journal = {Journal of Computational Physics},
	number = {1},
	pages = {35 - 60},
	title = {Combined Immersed-Boundary Finite-Difference Methods for Three-Dimensional Complex Flow Simulations},
	url = {http://www.sciencedirect.com/science/article/B6WHY-45FC8JY-4M/2/6b24ae68ad18e0ba9af4096f2e8a3117},

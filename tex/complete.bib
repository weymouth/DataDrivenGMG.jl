	journal = {Journal of Fluid Mechanics},
	pages = {1-28},
	title = {Three-dimensional flow structures and vorticity control in fish-like swimming},
	url = {http://dx.doi.org/10.1017/S002211200200143X},
	volume = {468},
	year = {2002},

	timestamp = {2010.06.11},
	title = {Direct numerical simulation of turbulent flow over a modeled riblet covered surface},
	url = {http://journals.cambridge.org/action/displayAbstract?fromPage=online&aid=340102&fulltextType=RA&fileId=S0022112095004125},

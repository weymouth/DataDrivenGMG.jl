	journal = {Harmful Algae},
	keywords = {Harmful algae},
	number = {1},
	pages = {71 - 80},
	title = {A simple model capable of simulating the population dynamics of Gambierdiscus, the benthic dinoflagellate responsible for ciguatera fish poisoning},
	url = {http://www.sciencedirect.com/science/article/B73D7-50KC6SB-1/2/40347abf71202b0a80eef3a4c0663fd1},
	volume = {10},

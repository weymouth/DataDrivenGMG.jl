	keywords = {ANN Workshop},
	number = {2-3},
	pages = {65 - 73},
	title = {Artificial neural networks as a tool in ecological modelling, an introduction},
	url = {http://www.sciencedirect.com/science/article/B6VBS-3X5GN3F-1/2/c1aaa1c481d31c67348163de92b32610},
	volume = {120},
	year = {1999},

	keywords = {Elliptic solvers},
	number = {19},
	pages = {7520 - 7544},
	title = {Efficient simulation of surface tension-dominated flows through enhanced interface geometry interrogation},
	url = {http://www.sciencedirect.com/science/article/pii/S0021999110003499},

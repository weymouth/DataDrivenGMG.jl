	journal = {Journal of Computational Physics},
	keywords = {Navier-Stokes equations},
	number = {7},
	pages = {2479 - 2496},
	title = {On the accuracy of direct forcing immersed boundary methods with projection methods},
	url = {http://www.sciencedirect.com/science/article/B6WHY-4XH5MS8-4/2/27dcfb0617a9b3d9a6b12f90ac27cbd3},
	volume = {229},
